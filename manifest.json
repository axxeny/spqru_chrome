{
  "name": "SPQRu",
  "description": "Have the Russian language content displayed with Latin letters",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://meduza.io/*",
        "https://ru.wikipedia.org/*"
      ]
    }
  ]
}
